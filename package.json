{
    "name": "hyde-language",
    "displayName": "Hyde Language",
    "description": "Syntax highlighting for Hyde, a Python interpretation of Lox",
    "version": "0.0.6",
    "publisher": "ty-porter",
    "author": "ty-porter",
    "icon": "images/CILogo.png",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "hyde",
                "aliases": [
                    "hy",
                    "hyde"
                ],
                "extensions": [
                    ".hy",
                    ".hyde"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "hyde",
                "scopeName": "source.hyde",
                "path": "./syntaxes/hyde.tmLanguage.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ty-porter/hyde-lang.git"
    },
    "keywords": [
        "hyde",
        "hyde-language",
        "vscode",
        "vscode-extension",
        "vscode-language"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ty-porter/hyde-lang/issues"
    },
    "homepage": "https://github.com/ty-porter/hyde-lang"
}
